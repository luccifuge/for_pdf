import pdfplumber
import pandas as pd

pdf = 'FS IFRS Rus 0624 disclosed.pdf'
csv= 'test.csv'

result = []

with pdfplumber.open(pdf) as pdf:

    for page in pdf.pages:
        tables = page.extract_tables()
        for i in tables:

            result.extend(i)

df = pd.DataFrame(result[1:], columns=result[0])
df.to_csv(csv, index=False)
